{
  "name": "vscode-react-clone",
  "version": "0.1.0-alpha",
  "description": "A clone of Visual Studio Code in React with AI-driven build system",
  "main": "dist/main.js",
  "repository": "git@github.com:bhongy/vscode-react-clone.git",
  "author": "Thanik Bhongbhibhat <email@thanik.me>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "test": "jest",
    "electron:build": "electron-webpack",
    "electron:dist": "electron-webpack && electron-builder",
    "electron:start": "cross-env NODE_OPTIONS=--openssl-legacy-provider electron-webpack dev",
    "electron:start:headless": "cross-env NODE_OPTIONS=--openssl-legacy-provider HEADLESS=true ELECTRON_NO_ATTACH_CONSOLE=true electron-webpack dev"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.11.1",
    "openai": "^3.2.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rxjs": "^6.5.4",
    "simple-git": "^3.27.0",
    "ts-morph": "^23.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^12.20.55",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "cross-env": "^7.0.3",
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "jest": "^25.1.3",
    "react-hot-loader": "^4.12.19",
    "ts-jest": "^25.2.1",
    "ts-node": "^10.9.1",
    "typescript": "4.5.5",
    "webpack": "^4.41.6"
  }
}
