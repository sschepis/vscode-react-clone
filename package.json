{
  "name": "vscode-react-clone",
  "version": "0.1.0-alpha",
  "description": "A clone of Visual Studio Code in React with AI-driven build system",
  "main": "dist/main.js",
  "repository": "git@github.com:bhongy/vscode-react-clone.git",
  "author": "Thanik Bhongbhibhat <email@thanik.me>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "start": "electron .",
    "dev": "vite",
    "test": "jest",
    "electron:build": "vite build && electron-builder",
    "electron:dev": "concurrently \"vite\" \"electron .\"",
    "electron:start": "electron .",
    "electron:start:headless": "cross-env HEADLESS=true ELECTRON_NO_ATTACH_CONSOLE=true electron ."
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "eslint": "^8.56.0",
    "openai": "^4.24.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.8.1",
    "simple-git": "^3.22.0",
    "ts-morph": "^23.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-electron": "^0.15.6"
  }
}
